{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ubc/api-datetime",
  "version": "1.0.0",
  "title": "Resource Datetime",
  "category": "theme",
  "description": "Display resource's datetime.",
  "ancestor": [
    "ubc/null"
  ],
  "attributes": {
    "dateType": {
      "type": "string",
      "default": ""
    },
    "textAlign": {
      "type": "string"
    },
    "format": {
      "type": "string"
    },
    "isLink": {
      "type": "boolean",
      "default": false
    }
  },
  "usesContext": [
    "datetimes",
    "link"
  ],
  "supports": {
    "html": false,
    "color": {
      "gradients": true,
      "link": true,
      "__experimentalDefaultControls": {
        "background": true,
        "text": true,
        "link": true
      }
    },
    "spacing": {
      "margin": true,
      "padding": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "__experimentalFontStyle": true,
      "__experimentalTextTransform": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalDefaultControls": {
        "fontSize": true
      }
    },
    "interactivity": {
      "clientNavigation": true
    }
  },
  "example": {},
  "textdomain": "ubc-wp-api-inner-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}